    "express": "^4.21.1",
    "graphql": "^16.9.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "request": "^2.88.2",
